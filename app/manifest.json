{
    "name": "Presently",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Chrome calendar app",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "presently",
    "default_popup": "tab.html"
  }
}
